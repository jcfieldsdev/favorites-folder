{
	"manifest_version": 3,
	"name": "Favorites Folder",
	"version": "1.1.1",
	"description": "A favorites browser.",
	"author": "J.C. Fields",
	"homepage_url": "https://github.com/jcfieldsdev/favorites-folder",
	"permissions": [
		"bookmarks",
		"favicon",
		"history",
		"storage"
	],
	"icons": {
		"16": "icons/icon-neutral-16.png",
		"32": "icons/icon-neutral-32.png",
		"48": "icons/icon-neutral-48.png",
		"64": "icons/icon-neutral-64.png"
	},
	"action": {
		"default_icon": {
			"16": "icons/icon-neutral-16.png",
			"32": "icons/icon-neutral-32.png",
			"48": "icons/icon-neutral-48.png",
			"64": "icons/icon-neutral-64.png"
		},
		"default_title": "Favorites Folder",
		"default_popup": "popup/favorites.html"
	}
}
